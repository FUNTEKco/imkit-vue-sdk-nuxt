    "vue-renderer-markdown": "0.0.61-beta.0",
    "vue-router": "^4.6.3",
    "vue3-emoji-picker": "^1.1.8",
    "vue3-google-map": "^0.22.0",
    "vue3-popper": "^1.5.0",
    "vue3-radial-progress": "^1.1.1",
    "vue3-touch-events": "^5.0.13",
    "vue3-virtual-scroll-list": "^0.2.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@eslint/js": "9.30.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tailwindcss/postcss": "^4.1.16",
    "@tailwindcss/typography": "^0.5.19",
    "@types/eslint": "^9.6.1",
    "@types/howler": "^2.2.12",
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/markdown-it": "^14.1.2",
    "@types/recordrtc": "^5.6.15",
    "@types/shuffle-array": "^1.0.5",
    "@types/throttle-debounce": "^5.0.2",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.4",
    "@types/video.js": "^7.3.58",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.21",
    "commitizen": "4.3.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^16.4.0",
    "husky": "9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "16.1.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass-embedded": "^1.93.2",
    "semantic-release": "24.2.5",
    "shuffle-array": "^1.0.1",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vite-plugin-libcss": "^1.1.2",
    "vite-plugin-mkcert": "^1.17.9",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^2.2.12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "packageManager": "pnpm@9.15.5"
}
