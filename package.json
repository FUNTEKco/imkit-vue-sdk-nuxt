    "browser-image-compression": "^2.0.2",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.18",
    "firebase": "^11.10.0",
    "get-blob-duration": "^1.2.0",
    "hash-wasm": "^4.12.0",
    "howler": "^2.2.4",
    "jose": "^6.1.0",
    "lucide-vue-next": "^0.544.0",
    "luxon": "^3.7.2",
    "markdown-it": "^14.1.0",
    "p-limit": "^6.2.0",
    "recordrtc": "^5.6.2",
    "runtime-memcache": "^3.1.0",
    "socket.io-client": "^4.8.1",
    "storage-available": "^1.1.0",
    "textarea-caret": "^3.1.0",
    "throttle-debounce": "^5.0.2",
    "uuid": "^11.1.0",
    "validator": "^13.15.20",
